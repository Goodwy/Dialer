    <!-- FAQ -->
    <string name="faq_1_title_dialer_g">מדוע אני צריך לבטל את נעילת המכשיר שלי כדי לענות לשיחה נכנסת?</string>
    <string name="faq_1_text_dialer_g">מערכת ההפעלה חוסמת גישה של אפליקציות למסך הנעילה. עליך לאפשר לאפליקציה לגשת למסך הנעילה. לדוגמה, ב-MIUI, עליך לאפשר גישה להגדרות => התראות => התראות במסך הנעילה.</string>
    <string name="faq_2_title_dialer_g">כיצד אוכל להעתיק או להדביק מספר במסך החייגן?</string>
    <string name="faq_2_text_dialer_g">לחץ לחיצה ארוכה על לחצן ההתקשרות כדי להדביק את תוכן הלוח. אם המספר כבר הוזן, לחיצה ארוכה על לחצן ההתקשרות תעתיק אותו ללוח.</string>

    <!-- סגנון תשובה -->
    <string name="faq_3_text_g">חייגן ימני מציג רק אנשי קשר שיש להם מספר טלפון המשויך אליהם. אנשי קשר ללא מספר טלפון אינם מוצגים באפליקציה.</string>
